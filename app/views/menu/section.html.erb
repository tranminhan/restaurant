<div class="row">

  <% @items.each do |item| %>
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <img src="<%= item.image_url %>" style="width: 243px; height: 200px;"alt="food">
        <div class="caption">
          <h3><%= item.name %></h3>
          <span><%= item.price %> VND</span>
          <p><%= item.description %></p>
          <p><a href="#" class="btn btn-primary" role="button">Order</a></p>
        </div>
      </div>
    </div>
  <% end %>
  
</div>